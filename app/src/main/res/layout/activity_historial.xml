<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="alberto.medicconsultapp.HistorialActivity">

    <TextView
        android:id="@+id/username"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/data"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/username"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/username" />

    <TextView
        android:id="@+id/nom"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.777"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/data" />

    <TextView
        android:id="@+id/cognoms"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/nom" />

    <TextView
        android:id="@+id/edat"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/cognoms" />

    <TextView
        android:id="@+id/enfermetat"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/edat" />

    <TextView
        android:id="@+id/observacions"
        android:layout_width="172dp"
        android:layout_height="33dp"
        android:layout_above="@+id/data"
        android:layout_alignStart="@+id/historial"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.777"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/enfermetat" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="27dp"
        android:text="Observacions"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="27dp"
        android:text="Enfermetat"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="27dp"
        android:text="Edat"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="27dp"
        android:text="Cognoms"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="30dp"
        android:text="Nom"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
            android:id="@+id/historial"
            android:layout_width="89dp"
            android:layout_height="33dp"
            android:layout_alignBaseline="@+id/username"
            android:layout_alignBottom="@+id/username"
        android:layout_toStartOf="@+id/data"
            android:text="@string/user_historial"
            android:layout_marginLeft="48dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="88dp"
        android:layout_height="33dp"
        android:layout_marginLeft="48dp"
        android:text="Data"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/historial" />
    <!--</LinearLayout>-->
</android.support.constraint.ConstraintLayout>
